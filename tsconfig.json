{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "hackerNews",
    "references",
    "cote"
  ]
}